<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>To Do List</title>
    <link rel="stylesheet" href="../css/styles.css" />
  </head>
  <body>
    <h1><%= kinfOfDay %></h1>
    <ul>
      <% newTodo.forEach(function(todo) { %>
        <li><%= todo %></li>
      <% }) %>
    </ul>
    <form action="/" method="post">
      <input type="text" name="newTodo" />
      <button type="submit">Add Todo</button>
    </form>
  </body>
</html>
